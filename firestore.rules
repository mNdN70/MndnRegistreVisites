rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /visits/{visitId} {
      allow read, write: if true;
    }
    match /departments/{departmentId} {
      allow read, write: if true;
    }
    match /employees/{employeeId} {
      allow read, write: if true;
    }
    match /users/{userId} {
      allow read, write: if true;
    }
  }
}